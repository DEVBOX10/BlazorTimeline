<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Title>BlazorTimeline</Title>
        <Authors>Morasiu</Authors>
        <Copyright>MIT</Copyright>
        <PackageProjectUrl>https://github.com/Morasiu/BlazorTimeline</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Morasiu/BlazorTimeline</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>blazor, timeline</PackageTags>
        <AssemblyVersion>1.0.0</AssemblyVersion>
    </PropertyGroup>
    <PropertyGroup>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <PropertyGroup>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\readme.md"/>
    </ItemGroup>
    
    
    <ItemGroup>
        <SupportedPlatform Include="browser" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.1" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="wwwroot" />
    </ItemGroup>

</Project>
